package JavaBaseClassLibrary;

public class ThrowTest
{
	public static void main(String[] args)
	{
		throwRuntime(3);
	}
	
	public static void throwRuntime(int a)
	{
		if(a>0)//使用throw在运行时抛出异常   属于RunTime异常体系，较checked异常体系简单一些。
			throw new RuntimeException("a is bigger than zero, not meet requirement...");
	}
}
